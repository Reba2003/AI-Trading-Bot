import alpaca_trade_api as tradeapi

DATA_FILE = "equities.json"

key = ""
secret_key = ""
BASE_URL = ""

api = tradeapi.REST(key, secret_key, BASE_URL, api_version="v2")

def get_data(symbol):
    try:
        barset = api.get_latest_trade(symbol)
        return {"price":barset.price}
    except Exception as e:
        return {"price":-1}